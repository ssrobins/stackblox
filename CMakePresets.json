{
    "version": 1,
    "cmakeMinimumRequired": {
      "major": 3,
      "minor": 19,
      "patch": 0
    },
    "configurePresets": [
      {
        "name": "androidarm",
        "generator": "Ninja Multi-Config",
        "binaryDir": "build_${name}",
        "cacheVariables": {
          "ANDROID_ABI": {
            "type": "STRING",
            "value": "armeabi-v7a"
          }
        }
      }
    ]
  }